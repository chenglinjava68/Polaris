<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mwclg.web.mapper.news.ColumnMapper">
    <resultMap id="BaseResultMap" type="com.mwclg.web.entity.po.Column">
        <id column="column_id" property="columnId" jdbcType="INTEGER"/>
        <result column="short_name" property="shortName" jdbcType="VARCHAR"/>
        <result column="column_num" property="columnNum" jdbcType="INTEGER"/>
        <result column="column_type" property="columnType" jdbcType="INTEGER"/>
        <result column="column_from" property="columnFrom" jdbcType="INTEGER"/>
        <result column="column_url" property="columnUrl" jdbcType="VARCHAR"/>
        <result column="column_name" property="columnName" jdbcType="VARCHAR"/>
        <result column="column_parent_id" property="columnParentId" jdbcType="INTEGER"/>
        <result column="column_top_id" property="columnTopId" jdbcType="INTEGER"/>
        <result column="column_level" property="columnLevel" jdbcType="INTEGER"/>
        <result column="column_name_en" property="columnNameEn" jdbcType="VARCHAR"/>
        <result column="templete_id" property="templeteId" jdbcType="INTEGER"/>
        <result column="create_by" property="createBy" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="update_by" property="updateBy" jdbcType="VARCHAR"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="modifiable" property="modifiable" jdbcType="INTEGER"/>
        <result column="req_mapping" property="reqMapping" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectAllColumnToWeb" resultMap="BaseResultMap">
	   select * from tbl_column  order by create_time  ASC
  	</select>

    <resultMap id="LatestUpdateTimeResultMap" type="com.mwclg.web.entity.po.Column">
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
    </resultMap>
    <select id="getLatestUpdateTime" resultMap="LatestUpdateTimeResultMap">
        select update_time from tbl_column  ORDER BY update_time  desc limit 1
    </select>
   
</mapper>